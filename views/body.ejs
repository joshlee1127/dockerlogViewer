<body class="container">
    <h1>Log view</h1>
    <div class="my-buttons">
        <% for(var i=0; i<names.length; i++) {%>
            <form action="/listalllog/<%= names[i]%>" method="GET">
                <div class="form-row form-row-end">
                    <button class="btn btn-primary" type="submit"><%= names[i] %></button>
                </div>
            </form>
        <% } %>
    </div>

    <div class="form-group">
        <p><%= logs%></p>
    </div>
</body>
